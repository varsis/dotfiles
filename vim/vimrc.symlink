function! FormatProject() 
  let g:ale_run_synchronously=1 
  if &filetype == "typescript" 
    execute "Ack --ts '.'"
  elseif &filetype == "javascript"
    execute "Ack --js '.'"
  else
    throw()
  endif 
  execute "cfdo ALEFix"
  execute "wall"
  execute "qall"
  let g:ale_run_synchronously=0
endfunction


